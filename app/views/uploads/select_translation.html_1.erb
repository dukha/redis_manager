<p class="pageheading"><%=t("headings.edit.heading", :model=>t($ARM + "translation", :count=>1) )%></p>


<%= semantic_form_for [@upload], :url => { :action => to_redis_path(params[:id]), :controller => "uploads"  } do |f| %> 
  <%=semantic_fields_for @database do |t| %>>
    <% t.inputs do  %>
      <%= t.input :id, :as=>:select,:collection=> Database.all.collect {|t| [ t.name, t.id ]} %>
    <% end %>
  <%end%>
  <%= f.buttons do %>
      <%= f.commit_button  :select%>
  <% end %>
 <%end%>
          

<%= link_to t('headings.new.heading', :model=> t($ARM + 'translation', :count=>1)), databases_path %> |
<%= link_to t("actions.back"), databases_path %>
