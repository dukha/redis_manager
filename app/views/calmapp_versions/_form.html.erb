<%= semantic_form_for(@calmapp_version) do |f| %>
  
	<%= f.inputs do %>
				   <%= f.input :calmapp_id, :as=>:select,:collection=> Calmapp.all.collect {|p| [ p.name, p.id ]}      %>
				   <%= f.input :version  %>
				   <%= f.input :release_status_id, :as=>:select,:collection=> ReleaseStatus.all.collect {|p| [ p.status, p.id ]}    %>
		      
	<% end %>	
	<%= f.buttons do %>
      <%= f.commit_button  %>    
  <% end %>
<% end %>

